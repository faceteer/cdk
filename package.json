    "build": "tsc -b --clean && tsc -b",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=14"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/faceteer/cdk.git"
